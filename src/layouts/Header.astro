---
import Container from "../components/basics/Container.astro";

interface Props {
  title: string;
}

const props = Astro.props;
---

<header class="h-16 w-full">
    <Container>
        <a href="/" class="text-lg font-medium uppercase tracking-widest">
            {props.title}
        </a>
    </Container>
</header>
